               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </GridPane>
            <Button fx:id="btn_signup" mnemonicParsing="false" prefHeight="26.0" prefWidth="124.0" text="SIGN UP">
               <VBox.margin>
                  <Insets bottom="10.0" left="100.0" right="100.0" top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
