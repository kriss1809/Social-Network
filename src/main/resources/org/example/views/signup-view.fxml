                  <PasswordField fx:id="input_parola" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <PasswordField fx:id="input_parola2" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <TextField fx:id="input_prenume" GridPane.columnIndex="1" GridPane.rowIndex="1" />
